{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".svelte-kit/output/static"
      }
    }
  ],
  "routes": [
    {
      "src": "^/courses/(.*)",
      "dest": "/api/courses/$1"
    },
    {
      "src": "^/(.*)",
      "dest": "/"
    }
  ]
}
